pipeline {
  #agent { label 'dev'}
  agent any
    stages {
        stage('Build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
                sh '/home/vagrant/maven/apache-maven-3.6.3/bin/mvn test'
            }
            post {
                always {
                    junit 'target/surefire-reports/*.xml'
                }
            }
        }
        stage('Deliver') {
           steps {
               sh './jenkins/scripts/deliver.sh'
            }
        }
    }
